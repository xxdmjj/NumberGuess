import random

from random import randrange


LowerLeg = 1
UpperLeg = 100


def Range_(LowerLeg, UpperLeg):
    randrange(LowerLeg, UpperLeg)


def user_number():
    input('Pick a number')
    return user_number()


def TooHigh():
    print('TooHigh')


def TooLow():
    print('TooLow')


num = user_number()


if num > Range_(): TooLow(): 
    elif num < Range_() TooHigh():

else print("Number guessed correctly")
